<Application x:Class="ColorPipette.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ColorPipette"
             xmlns:vm="clr-namespace:ColorPipette.ViewModels"
             xmlns:conv="clr-namespace:ColorPipette.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"/>
            <conv:StartPositionTopConverter x:Key="startPositionTopConverter"/>
            <conv:StartPositionLeftConverter x:Key="startPositionLeftConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
